{
  "name": "jellysplit",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:dev:client": "yarn workspace @jellysplit/client start:dev",
    "start:dev:server": "yarn workspace @jellysplit/server start:dev",
    "start:prod:client": "yarn workspace @jellysplit/client start:prod",
    "start:prod:server": "yarn workspace @jellysplit/server start:prod"
  }
}
